{
  "name": "esbuild",
  "version": "1.0.0",
  "description": "experimenting with esbuild :)",
  "main": "index.js",
  "scripts": {
    "build": "esbuild ./src/index.jsx --bundle --outfile=./src/dist/index.js --loader:.js=jsx",
    "watch": "esbuild ./src/index.jsx --bundle --outfile=./src/dist/index.js --watch",
    "serve": "esbuild ./src/index.jsx --bundle --outfile=./src/dist/index.js --servedir=./src --serve=8000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "esbuild": "^0.25.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
